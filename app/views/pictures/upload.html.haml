= form_for @picture, html: { multipart: true } do |f|
  = f.label :photo
  = f.file_field :photo, multiple: true, name: 'picture[photo]'
  = f.submit 'Save', class: 'button'

#uploadingPhotos

%script#template-upload{type: "text/x-tmpl"}
  :cdata
    <div class="upload">{%=o.name%}<div class="progress"><span class="meter" style="width: 0%"></span></div></div>
